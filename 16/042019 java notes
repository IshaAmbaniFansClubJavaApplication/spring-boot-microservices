package spring.boot.microservices.info.springbootmicroservicmovieinfoapp.resources;

import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import spring.boot.microservices.info.springbootmicroservicmovieinfoapp.models.Movie;

@RestController
@RequestMapping("/movies")
public class MovieResource {
	
	@RequestMapping("/{movieId}")
	public Movie getMovieInfo(@PathVariable("movieId") String movieId) {
		return new Movie(movieId,"Test name");
	}

}
package spring.boot.microservices.rating.springbootmicroservicmovieratingapp.model;

public class Rating {
private String movieId;
private int rating;
public Rating(String movieId, int rating) {
	this.movieId=movieId;
	this.rating=rating ;
}
public String getMovieId() {
	return movieId;
}
public void setMovieId(String movieId) {
	this.movieId = movieId;
}
public int getRating() {
	return rating;
}
public void setRating(int rating) {
	this.rating = rating;
}

}

package spring.boot.microservices.rating.springbootmicroservicmovieratingapp.resources;

import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import spring.boot.microservices.rating.springbootmicroservicmovieratingapp.model.Rating;

@RestController
@RequestMapping("/ratingsdata")
public class RatingResource {

	@RequestMapping("/{movieId}")
	public Rating getRating(@PathVariable("movieId") String movieId) {
		return new Rating(movieId,4);
	}
}

input to browser :http://localhost:7777/ratingsdata/1234
OutPut : {"movieId":"1234","rating":4}

above three application are now not a spring boot microservices but here only three independent  applcations only that applicaations related to normal spring boot applications
 
above having 3 spring boot applicaations but not having any relationship of the any applicaation 

