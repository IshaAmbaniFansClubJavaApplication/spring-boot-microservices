import java.util.HashMap;
import java.util.Map;
import java.util.Map.Entry;

public class EmpployeeClient {
	public static void main(String[] args) {
		// we can get the result
		Map<Integer, String> putValueesMap1 = new HashMap<>();
		putValueesMap1.put(1001, "J2SE");
		putValueesMap1.put(1002, "J2EE");
		putValueesMap1.put(1003, "JDBC");
		putValueesMap1.put(1004, "Spring Framework");
		putValueesMap1.put(1005, "Spring Boot");
		System.out.println(putValueesMap1.get(1003));
		// by using the Entry we can retrive the elements
		for(Map.Entry retriveMapElements : putValueesMap1.entrySet()) {
			putValueesMap1.put(1006,"Raja");// here we can add the one element in the middle of the retriving time that's why we will get error at runn timme but not compile time of the our applcation 
			System.out.println("get key values	:"+retriveMapElements.getKey()+"	get value  :"+retriveMapElements.getValue());
		}
		
	}
}



