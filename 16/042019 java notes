LoginServlet.java
-------------------------		   
		   
public class LoginServlet extends HttpServlet {

public void doPost(HttpServletRequest request,HttpServletResponse response) throws ServletException,IOException{
respone.setContentType("text/html");
PwintWriter out = respone.getWriter();
 String uname = request.getParameter("uname");
 String pword = request.getParameter("pword");
  try{
  Class.forName("oracle.jdbc.driver.OracleDriver");
  Connection con = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:XE","system","admin");
  PreparedStatement ps = con.prepareStatement("select *from Login where uname=? and pword=?");
  ps.setString(1,uname);
  ps.setString(2,pword);
  ResultSet rs = ps.executeQuery();
  if(rs.next()){
  pw.println("Login Successful");
  RequestDispatcher rd = req.getRequestDispatcher("dis");// this is URL of the applicaion second servlet /html/jsp page any page it ccan hit this is matching the url-pattern of the wex.xml page of the application
  rd.forword(request,response);// this result are go to the second page and shows the result of the page of the application 
  }//end of if condition
   else{
   out.println("invalid UserLogin");
   RequestDispatcher rd = requestgetRequestDispatcher("login.html").include(request,response); //here display the 
  result of the same page but not go to the another page or another servlet program only page it self showing the result
or
RequestDispatcher rd = requestgetRequestDispatcher("login.html");//here whatever page it can be give it can be hit the that perticular page of the applcation 
rd.include(request,response); //here display the  
   }
  }
  }
  
  
