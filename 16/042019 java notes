package com.shift.operations;

public class LeftshiftOpretor {
	public static void main(String[] args) {
		int x = 2;
		System.out.println(x<<1);
		System.out.println(x<<2);
		System.out.println(x<<3);
  // what is use of listshift operator anaga ikkada manaku 	
