 import { Component } from '@angular/core';

@Component({
  selector: 'app-root',// here selector is a tag name for this component here what tag name am used in the selector of the application  <this app-root> having to index.html page also 
  //in index.html page having the tag is <app-root></app-root> 
  templateUrl: './app.component.html',// this is template url this one is where the location of the html page and also name as the html page  
  styleUrls: ['./app.component.css']// here styling as the specific component of the application 
})
export class AppComponent {
  title = 'spring-boot-rest-user-registration-form';// this value is means the title values it can be mapped bby app.component.html page that means app.component.html page show to print these values {{title}} we can print the values of the title not only title we can give any values of the application that values are printed here only
that's called as a interpolation-{{data}}  


@Component({
  selector: 'app-root',
  /** templateUrl: './app.component.html',*/
     templateUrl: `<h1>{{title}}</h1>`,
  styleUrls: ['./app.component.css']
})
export class AppComponent {
  title = 'spring-boot-rest-user-registration-form';
  welcome = 'Welcome to Angular Application ';

}
