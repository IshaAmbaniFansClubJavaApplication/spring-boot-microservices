		// we can get the result
		Map<Integer, String> putValueesMap = new HashMap<>();
		putValueesMap.put(1001, "J2SE");
		putValueesMap.put(1002, "J2EE");
		putValueesMap.put(1003, "JDBC");
		putValueesMap.put(1004, "Spring Framework");
		putValueesMap.put(1005, "Spring Boot");
